      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fairy.weather"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "74cd7eac-62eb-4392-b435-c4ceb34af7bc"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
